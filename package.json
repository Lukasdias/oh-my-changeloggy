{
  "name": "oh-my-changeloggy",
  "version": "1.2.7",
  "description": "Generate changelogs from git conventional commits",
  "module": "dist/index.js",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "changeloggy": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run src/cli.ts",
    "build": "bun run build.ts",
    "start": "bun dist/cli.js",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "release": "./scripts/release.sh"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "commander": "^12.0.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.11",
    "bun-types": "latest",
    "typescript": "^5.0.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "changelog",
    "git",
    "release-notes",
    "cli",
    "conventional-commits",
    "developer-tools"
  ],
  "author": "Lukasdias",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Lukasdias/oh-my-changeloggy.git"
  },
  "bugs": {
    "url": "https://github.com/Lukasdias/oh-my-changeloggy/issues"
  },
  "homepage": "https://github.com/Lukasdias/oh-my-changeloggy#readme"
}
