{
  "name": "oh-my-changeloggy",
  "version": "1.2.4",
  "description": "Generate user-friendly changelogs from git commits with interactive TUI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "changeloggy": "dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outfile ./dist/index.js --target node && echo '#!/usr/bin/env node' | cat - ./dist/index.js > temp && mv temp ./dist/index.js && chmod +x ./dist/index.js",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "prepublishOnly": "bun run build && bun run build:types"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "commander": "^12.0.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.11",
    "bun-types": "latest",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "changelog",
    "git",
    "release-notes",
    "cli",
    "conventional-commits",
    "tui",
    "interactive",
    "semver",
    "versioning",
    "developer-tools"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lukasdias/oh-my-changeloggy.git"
  },
  "bugs": {
    "url": "https://github.com/Lukasdias/oh-my-changeloggy/issues"
  },
  "homepage": "https://github.com/Lukasdias/oh-my-changeloggy#readme"
}
